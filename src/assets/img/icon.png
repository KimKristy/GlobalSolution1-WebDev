  </span><span><span class="hljs-attribute">color</span></span><span>: </span><span><span class="hljs-built_in">var</span></span><span>(--cor-texto);
  </span><span><span class="hljs-attribute">background</span></span><span>: </span><span><span class="hljs-built_in">linear-gradient</span></span><span>(</span><span><span class="hljs-built_in">rgba</span></span><span>(</span><span><span class="hljs-number">255</span></span><span>,</span><span><span class="hljs-number">255</span></span><span>,</span><span><span class="hljs-number">255</span></span><span>,</span><span><span class="hljs-number">0.85</span></span><span>), </span><span><span class="hljs-built_in">rgba</span></span><span>(</span><span><span class="hljs-number">255</span></span><span>,</span><span><span class="hljs-number">255</span></span><span>,</span><span><span class="hljs-number">255</span></span><span>,</span><span><span class="hljs-number">0.85</span></span><span>)), 
              </span><span><span class="hljs-built_in">url</span></span><span>(</span><span><span class="hljs-string">"../img/fundo.jpg"</span></span><span>) no-repeat center center fixed;
  </span><span><span class="hljs-attribute">background-size</span></span><span>: cover;
  </span><span><span class="hljs-attribute">line-height</span></span><span>: </span><span><span class="hljs-number">1.6</span></span><span>;
